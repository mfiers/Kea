- cramtools
- zeroormore:
  - - cram
    - zeroormore:
      - - "--capture-tags"
        - '/.[QC]+./'
      - "--capture-all-tags"
      - "--encrypt"
      - - "-I"
        - named:
            name: input
            value: "/.*.bam/"
  - - bam
    - zeroormore:
      - oneof: ["--password", "-p"]
      - - oneof: ["-f", "-required-flags"]
        - "re#\\w+"
  - ""
- "sp#$"
